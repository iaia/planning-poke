<!DOCTYPE html>
<html>
  <head>
    <title>PlanningPoke</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_pack_tag 'application' %>

  </head>

  <body>
    <nav>
      <div class="nav-wrapper">
        <div class="brand-logo">
          Planning-Poke
        </div>
        <ul class="right">
          <li><%= content_for?(:user_name) ? yield(:user_name) : "" %></li>
          <li><%= content_for?(:room_name) ? yield(:room_name) : "" %></li>
          <li>nav微妙</li>
        </ul>
      </div>
    </nav>

    <div class="container">
      <div class="row">
        <%= yield %>
      </div>
    </div>
  </body>
</html>
